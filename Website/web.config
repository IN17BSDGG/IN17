            <error statusCode="508" path="/error/error.php?statuscode=508" responseMode="ExecuteURL" />
            <error statusCode="509" path="/error/error.php?statuscode=509" responseMode="ExecuteURL" />
            <error statusCode="510" path="/error/error.php?statuscode=510" responseMode="ExecuteURL" />
            <error statusCode="511" path="/error/error.php?statuscode=511" responseMode="ExecuteURL" />
            <error statusCode="400" path="/error/error.php?statuscode=400" responseMode="ExecuteURL" />
            <error statusCode="404" path="/error/error.php?statuscode=404" responseMode="ExecuteURL" />
            <error statusCode="407" path="/error/error.php?statuscode=407" responseMode="ExecuteURL" />
            <error statusCode="408" path="/error/error.php?statuscode=408" responseMode="ExecuteURL" />
            <error statusCode="409" path="/error/error.php?statuscode=409" responseMode="ExecuteURL" />
