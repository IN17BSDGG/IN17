<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpErrors>
            <remove statusCode="502" subStatusCode="-1" />
            <remove statusCode="501" subStatusCode="-1" />
            <remove statusCode="500" subStatusCode="-1" />
            <remove statusCode="412" subStatusCode="-1" />
            <remove statusCode="406" subStatusCode="-1" />
            <remove statusCode="405" subStatusCode="-1" />
            <remove statusCode="403" subStatusCode="-1" />
            <remove statusCode="404" subStatusCode="-1" />
            <remove statusCode="401" subStatusCode="-1" />
            <error statusCode="401" path="/error/error.php?statuscode=401" responseMode="ExecuteURL" />
            <error statusCode="403" path="/error/error.php?statuscode=403" responseMode="ExecuteURL" />
            <error statusCode="405" path="/error/error.php?statuscode=405" responseMode="ExecuteURL" />
            <error statusCode="406" path="/error/error.php?statuscode=406" responseMode="ExecuteURL" />
            <error statusCode="412" path="/error/error.php?statuscode=412" responseMode="ExecuteURL" />
