            <error statusCode="417" path="/error/error.php?statuscode=417" responseMode="ExecuteURL" />
            <error statusCode="420" path="/error/error.php?statuscode=420" responseMode="ExecuteURL" />
            <error statusCode="421" path="/error/error.php?statuscode=421" responseMode="ExecuteURL" />
